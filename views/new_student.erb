<form action="/students" method="post">
  <!--may have to change the route...come back to this  -->
<label for="first-name-field">First Name: </label>
<input
type="text"
name="first_name"
id="first-name-field"
/>

<label for="second-name-field">Second Name: </label>
<input
type="text"
name="second_name"
id="second-name-field"
/>

<label for="house-field">Select a House</label>
<select
name="house"
id="house-field"
/>
<option value="1">Griffindor</option>
<option value="2">Hufflepuff</option>
<option value="3">Ravenclaw</option>
<option value="4">Slytherin</option>
</select>

<label for="age-field">Age: </label>
<input
type="number"
name="age"
id="age-field"
/>


<input type="submit" value="Enroll Student">

</form>
